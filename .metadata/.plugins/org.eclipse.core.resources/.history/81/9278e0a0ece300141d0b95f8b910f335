package com.totvs.hcm.tsa.markgenerator.ui.events;

import java.util.Date;

public class SimulateProcessEvent {
	private SimulateProcessType typeEnum;
	private Date date;

	public SimulateProcessEvent(SimulateProcessType typeEnum) {
		super();
		this.typeEnum = typeEnum;
		this.date = new Date();
	}

	public SimulateProcessType getTypeEnum() {
		return typeEnum;
	}

	public void setTypeEnum(SimulateProcessType typeEnum) {
		this.typeEnum = typeEnum;
	}

	public Date getDate() {
		return date;
	}

	public void setDate(Date date) {
		this.date = date;
	}

	public enum SimulateProcessType {
		STARTED, CANCELED, CONCLUDED;
	}
}
